        {
          "_id": "recipe04",
          "dishName": "BBQ Pulled Pork Sandwiches",
          "ingredients": [
            { "name": "Pork shoulder", "quantity": "3 lbs" },
            { "name": "BBQ sauce", "quantity": "2 cups" },
            { "name": "Hamburger buns", "quantity": "8" },
            { "name": "Coleslaw", "quantity": "2 cups" },
            { "name": "Salt", "quantity": "1 tsp" },
            { "name": "Pepper", "quantity": "1/4 tsp" }
          ],
          "instructions": [
            "Preheat oven to 325°F.",
            "Season pork shoulder with salt and pepper, and place in a roasting pan.",
            "Cover with foil and roast for 4-5 hours, or until tender.",
            "Remove pork from oven and let cool.",
            "Shred pork with two forks.",
            "In a large bowl, mix shredded pork with BBQ sauce.",
            "Toast hamburger buns in the oven for 1-2 minutes.",
            "Assemble sandwiches by placing pulled pork on buns, and topping with coleslaw."
          ]
        },
        {
          "_id": "recipe05",
          "dishName": "Fried Chicken",
          "ingredients": [
            { "name": "Chicken pieces", "quantity": "8" },
            { "name": "Buttermilk", "quantity": "2 cups" },
            { "name": "Flour", "quantity": "2 cups" },
            { "name": "Salt", "quantity": "1 tbsp" },
            { "name": "Pepper", "quantity": "1 tsp" },
            { "name": "Paprika", "quantity": "1 tsp" },
            { "name": "Garlic powder", "quantity": "1 tsp" },
            { "name": "Vegetable oil", "quantity": "4 cups" }
          ],
          "instructions": [
            "In a large bowl, marinate chicken pieces in buttermilk for at least 2 hours.",
            "In a separate bowl, mix flour, salt, pepper, paprika, and garlic powder.",
            "Remove chicken from buttermilk and dredge in the flour mixture.",
            "Heat vegetable oil in a large skillet over medium-high heat.",
            "Fry chicken pieces for 10-15 minutes, or until fully cooked and crispy.",
            "Place fried chicken on a wire rack to cool and drain excess oil."
          ]
        }
      ]
